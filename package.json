{
  "name": "refactor-api-saef-comanda",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/iago-f-s-e/refactor-api-saef-comanda.git",
  "author": "iago fagundes <iago.fagundes@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "yarn build && yarn start:prod",
    "start:prod": "env NODE_ENV=production node dist/index.js",
    "start:dev": "env NODE_ENV=test ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "env NODE_ENV=test jest --detectOpenHandles",
    "test:functional": "env NODE_ENV=test jest --projects ./test --runInBand",
    "typeorm:run": "env NODE_ENV=test ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run",
    "typeorm:revert": "env NODE_ENV=test ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert",
    "typeorm:generate": "env NODE_ENV=test ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -n"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mssql": "^7.2.1",
    "node-cmd": "^5.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@babel/plugin-transform-flow-strip-types": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "15.12.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.2.4",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
